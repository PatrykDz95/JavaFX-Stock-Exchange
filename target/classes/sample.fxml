<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="496.0" prefWidth="832.0">
         <children>
            <Label fx:id="label" layoutX="666.0" prefHeight="54.0" prefWidth="166.0" />
            <PieChart fx:id="pieChart" layoutX="480.0" layoutY="146.0" prefHeight="213.0" prefWidth="297.0" />
            <BarChart fx:id="barChart" layoutX="8.0">
              <xAxis>
                <CategoryAxis fx:id="yAxis" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" fx:id="xAxis" />
              </yAxis>
            </BarChart>
            <VBox layoutX="697.0" prefHeight="496.0" prefWidth="135.0" styleClass="sidebar" stylesheets="@stylesheet.css">
               <children>
                  <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="136.0" styleClass="sidebar" stylesheets="@stylesheet.css" text="Bar Chart" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Button>
               </children></VBox>
         </children></Pane>
   </children>
</GridPane>
